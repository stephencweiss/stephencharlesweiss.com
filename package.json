{
    "name": "stephencharlesweiss.com",
    "private": true,
    "description": "A personal blog using Gatsby and Markdown",
    "version": "0.1.0",
    "author": "Stephen Weiss <stephen.c.weiss@gmail.com>",
    "dependencies": {
        "@gatsby-contrib/gatsby-plugin-elasticlunr-search": "^2.2.1",
        "cloc": "^2.5.1",
        "dayjs": "^1.8.19",
        "gatsby": "^2.29.3",
        "gatsby-image": "^2.2.39",
        "gatsby-plugin-feed": "^2.3.26",
        "gatsby-plugin-manifest": "^2.2.39",
        "gatsby-plugin-netlify": "^2.3.13",
        "gatsby-plugin-netlify-cache": "^1.2.0",
        "gatsby-plugin-offline": "^3.2.7",
        "gatsby-plugin-react-helmet": "^3.1.21",
        "gatsby-plugin-sharp": "^2.6.35",
        "gatsby-plugin-styled-components": "^3.1.18",
        "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
        "gatsby-remark-autolink-headers": "^2.1.23",
        "gatsby-remark-copy-linked-files": "^2.1.36",
        "gatsby-remark-embed-snippet": "^4.1.21",
        "gatsby-remark-embedder": "^2.1.1",
        "gatsby-remark-images": "^3.3.8",
        "gatsby-remark-prismjs": "^3.3.30",
        "gatsby-remark-reading-time": "^1.1.0",
        "gatsby-remark-responsive-iframe": "^2.2.31",
        "gatsby-remark-smartypants": "^2.1.20",
        "gatsby-source-filesystem": "^2.1.46",
        "gatsby-source-xkcd": "^1.0.0",
        "gatsby-transformer-remark": "^2.7.1",
        "gatsby-transformer-sharp": "^2.3.13",
        "prismjs": "^1.27.0",
        "react": "^16.5.1",
        "react-dom": "^16.5.1",
        "react-helmet": "^6.0.0",
        "remark-grid-tables": "^2.1.1",
        "styled-theming": "^2.2.0",
        "use-debounce": "^3.1.0"
    },
    "devDependencies": {
        "@babel/plugin-proposal-optional-chaining": "^7.8.3",
        "@babel/plugin-transform-react-jsx-development": "^7.9.0",
        "@commitlint/cli": "^12.1.1",
        "@commitlint/config-conventional": "^8.3.4",
        "babel-plugin-styled-components": "^1.10.7",
        "babel-preset-gatsby": "^0.4.7",
        "eslint": "^7.1.0",
        "eslint-config-prettier": "^7.1.0",
        "eslint-plugin-react": "^7.18.0",
        "gatsby-remark-code-titles": "^1.1.0",
        "husky": "^4.2.1",
        "netlify-cli": "^2.59.1",
        "prettier": "^2.0.5",
        "pretty-quick": "^2.0.1",
        "styled-components": "^5.1.1",
        "styled-system": "^5.1.4",
        "stylelint": "^13.13.1",
        "stylelint-config-recommended": "^3.0.0",
        "stylelint-config-styled-components": "^0.1.1",
        "stylelint-processor-styled-components": "^1.9.0"
    },
    "keywords": [
        "gatsby",
        "blog"
    ],
    "license": "MIT",
    "main": "n/a",
    "scripts": {
        "analyze": "ANALYZE_BUNDLE_SIZE=true gatsby build",
        "build": "npm run pre-build && gatsby build",
        "clean": "rm -rf .cache && rm -rf public",
        "dev": "gatsby develop",
        "develop": "npm run dev",
        "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix gatsby-node.js",
        "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md'",
        "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
        "lint:css": "stylelint './src/**/*.jsx?'",
        "nofilter": "export nofilter='true' && npm run dev",
        "preview": "npm run nofilter",
        "start": "npm run develop",
        "pre-build": "npm run clean && npm run site-stats",
        "publish:site": "netlify deploy",
        "site-stats": "cloc . --exclude-dir=$(tr '\n' ',' < .clocignore) --md > ./content/stats/current-stats.md",
        "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
        "updt:content": "git submodule update --remote"
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
            "pre-commit": "pretty-quick --staged"
        }
    },
    "volta": {
        "node": "12.18.3",
        "yarn": "1.22.5"
    }
}
