{
    "name": "stephencharlesweiss.com",
    "private": true,
    "description": "A personal blog using Gatsby and Markdown",
    "version": "0.1.0",
    "author": "Stephen Weiss <stephen.c.weiss@gmail.com>",
    "dependencies": {
        "@gatsby-contrib/gatsby-plugin-elasticlunr-search": "^2.2.1",
        "dayjs": "^1.8.19",
        "gatsby": "^2.19.7",
        "gatsby-image": "^2.2.39",
        "gatsby-plugin-facebook-pixel": "^1.0.3",
        "gatsby-plugin-feed": "^2.3.26",
        "gatsby-plugin-google-analytics": "^2.1.34",
        "gatsby-plugin-manifest": "^2.2.39",
        "gatsby-plugin-netlify-cache": "^1.2.0",
        "gatsby-plugin-offline": "^2.0.5",
        "gatsby-plugin-react-helmet": "^3.1.21",
        "gatsby-plugin-sharp": "^2.4.3",
        "gatsby-plugin-styled-components": "^3.1.18",
        "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
        "gatsby-remark-autolink-headers": "^2.1.23",
        "gatsby-remark-copy-linked-files": "^2.1.36",
        "gatsby-remark-embed-snippet": "^4.1.21",
        "gatsby-remark-embed-video": "^1.7.0",
        "gatsby-remark-images": "^2.0.4",
        "gatsby-remark-prismjs": "^3.3.30",
        "gatsby-remark-responsive-iframe": "^2.2.31",
        "gatsby-remark-smartypants": "^2.1.20",
        "gatsby-source-filesystem": "^2.1.46",
        "gatsby-transformer-remark": "^2.6.48",
        "gatsby-transformer-sharp": "^2.3.13",
        "nuka-carousel": "^4.6.1",
        "prismjs": "^1.19.0",
        "react": "^16.5.1",
        "react-dom": "^16.5.1",
        "react-helmet": "^5.2.0",
        "react-use": "^13.22.2",
        "remark-grid-tables": "^1.6.1",
        "styled-theming": "^2.2.0",
        "typeface-merriweather": "0.0.43",
        "typeface-montserrat": "0.0.43",
        "use-debounce": "^3.1.0"
    },
    "devDependencies": {
        "@babel/plugin-proposal-optional-chaining": "^7.8.3",
        "@commitlint/cli": "^8.3.5",
        "@commitlint/config-conventional": "^8.3.4",
        "babel-plugin-styled-components": "^1.10.7",
        "babel-preset-gatsby": "^0.2.27",
        "eslint": "^4.19.1",
        "eslint-plugin-react": "^7.18.0",
        "gatsby-remark-code-titles": "^1.1.0",
        "husky": "^4.2.1",
        "prettier": "^1.14.2",
        "styled-components": "^4.4.1",
        "styled-system": "^5.1.4",
        "stylelint": "^12.0.1",
        "stylelint-config-recommended": "^3.0.0",
        "stylelint-config-styled-components": "^0.1.1",
        "stylelint-processor-styled-components": "^1.9.0"
    },
    "keywords": [
        "gatsby",
        "blog"
    ],
    "license": "MIT",
    "main": "n/a",
    "scripts": {
        "analyze": "ANALYZE_BUNDLE_SIZE=true gatsby build",
        "build": "gatsby build",
        "clean": "rm -rf .cache && rm -rf public",
        "dev": "gatsby develop",
        "develop": "npm run dev",
        "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix gatsby-node.js",
        "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md'",
        "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
        "lint:css": "stylelint './src/**/*.jsx?'",
        "nofilter": "nofilter='true' npm run dev",
        "preview": "npm run nofilter",
        "start": "npm run develop",
        "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\""
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    }
}